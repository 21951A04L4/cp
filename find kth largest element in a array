import heapq
class Solution:
    def findKthLargest(self, arr: List[int], k: int) -> int:
        arr=[-num for num in arr]
        heapq.heapify(arr)
        for x in range(k):
            d=heapq.heappop(arr)
        return -d
Input: nums = [3,2,1,5,6,4], k = 2
Output: 5
Example 2:

Input: nums = [3,2,3,1,2,4,5,5,6], k = 4
Output: 
